[remote]
scylla ansible_host=192.168.0.120 user=root ansible_ssh_private_key_file=~/.ssh/ansible
charybdis ansible_host=192.168.0.121 user=root ansible_ssh_private_key_file=~/.ssh/ansible

[remote_admin]
scylla ansible_host=192.168.0.120 user=admin ansible_ssh_private_key_file=~/.ssh/ansible_admin
charybdis ansible_host=192.168.0.121 user=admin ansible_ssh_private_key_file=~/.ssh/ansible_admin
